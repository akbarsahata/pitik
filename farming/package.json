    "husky": "^7.0.4",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "lint-staged": "^13.2.1",
    "prettier": "^2.5.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "sonarqube-scanner": "^3.1.0",
    "ts-jest": "^26.5.6",
    "ts-node-dev": "^2.0.0"
  },
