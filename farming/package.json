{
  "name": "farming",
  "version": "2.81.1-layer_app-1",
  "description": "Pitik's backend service related to farming activities and such",
  "main": "src/index.ts",
  "engines": {
    "node": "18.12.0"
  },
  "scripts": {
    "test": "jest",
    "test:controller": "jest ./api --verbose --forceExit",
    "test:dao": "jest ./dao --verbose --forceExit",
    "test:service": "jest ./services --verbose --forceExit",
    "prepare": "husky install",
    "dev": "NODE_ENV=development NODE_NO_WARNINGS=1 ts-node-dev --rs -r dotenv/config src/index.ts",
    "debug": "NODE_ENV=development NODE_NO_WARNINGS=1 ts-node-dev --rs --inspect -r dotenv/config src/index.ts",
    "start": "pm2 start",
    "build": "tsc",
    "container": "node -r dotenv/config --expose-gc --max-old-space-size=2048 dist/src/index.js",
    "production": "tsc && pm2 start && pm2 save --force",
    "scan": "sonar-scanner"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/pitik.id/backend/farming.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/pitik.id/backend/farming/issues"
  },
  "homepage": "https://gitlab.com/pitik.id/backend/farming#readme",
